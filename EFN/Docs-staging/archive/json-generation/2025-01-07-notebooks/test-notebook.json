          "test-radio-group",
          "test-checkbox",
          "test-advanced-select"
        ]
      },
      "section-datetime-fields": {
        "label": "Date & Time Fields",
        "fields": [
          "test-date-picker",
          "test-time-picker",
          "test-datetime-picker",
          "test-datetime-now"
        ]
      },
      "section-number-fields": {
        "label": "Number Fields",
        "fields": [
          "test-integer",
          "test-float",
          "test-controlled-number"
        ]
      },
      "section-location-fields": {
        "label": "Location Fields",
        "fields": [
          "test-take-point",
          "test-map-field"
        ]
      },
      "section-media-fields": {
        "label": "Media Fields",
        "fields": [
          "test-take-photo",
          "test-file-uploader",
          "test-audio-recorder",
          "test-video-recorder"
        ]
      },
      "section-special-fields": {
        "label": "Special Fields",
        "fields": [
          "test-qrcode",
          "test-rich-text",
          "test-related-record",
          "test-address"
        ]
      },
      "section-conditional-logic": {
        "label": "Conditional Logic",
        "fields": [
          "test-condition-trigger",
          "test-conditional-text",
          "test-conditional-number",
          "test-conditional-media",
          "test-complex-condition"
        ]
      },
      "section-meta-properties": {
        "label": "Meta Properties",
        "fields": [
          "test-meta-annotation",
          "test-meta-uncertainty"
        ]
      }
    },
    "viewsets": {
      "viewset-basic": {
        "label": "Basic Fields",
        "views": [
          "section-text-fields",
          "section-selection-fields",
          "section-datetime-fields",
          "section-number-fields"
        ]
      },
      "viewset-advanced": {
        "label": "Advanced Fields",
        "views": [
          "section-location-fields",
          "section-media-fields",
          "section-special-fields"
        ]
      },
      "viewset-features": {
        "label": "Features & Logic",
        "views": [
          "section-conditional-logic",
          "section-meta-properties"
        ]
      }
    },
    "visible_types": [
      "viewset-basic",
      "viewset-advanced",
      "viewset-features"
    ]
  }
}